#include<stdio.h>
  int main()
  {
      int num,a,b;
    
      printf("\n enter the number :");
      scanf("%d",&num);
      a=num %5;
      b=num %11;
      if(a&&b==0){
      printf("given number is divisible by 5 and 11");
          
      }
      else
      {
      printf("givin number is not divisible by 5 and 11");
      }
     return 0; 
  }

