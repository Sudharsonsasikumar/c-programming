#include<stdio.h>
    int main()
    {
    int n;
    printf("enter the value of n:",n);
    scanf("%d",&n);
    if(n/2==0)
    {
        printf("\neven");
        
    }else{
        printf("\nodd");
    }
    
    return 0;
    }
